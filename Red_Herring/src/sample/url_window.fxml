<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.RowConstraints?>

<GridPane onKeyPressed="#handleKeyInput" alignment="center" xmlns="http://javafx.com/javafx/8.0.172-ea"
          xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.Url_Window_Controller" hgap="10" vgap="10">
   <children>
      <TextField fx:id="projectField" GridPane.columnIndex="2" GridPane.rowIndex="0" GridPane.halignment="CENTER" GridPane.valignment="CENTER" />
      <TextField fx:id="urlField" GridPane.columnIndex="2" GridPane.rowIndex="1" GridPane.halignment="CENTER" GridPane.valignment="CENTER" />
      <Label text="Project name :" GridPane.rowIndex="0"/>
      <Label text="Enter Url :" GridPane.rowIndex="1"/>
      <Button fx:id="generateButton" onAction="#handleGenerateButtonAction" mnemonicParsing="false" text="Generate" GridPane.columnIndex="3" GridPane.rowIndex="1"/>
   </children>
   
   <columnConstraints>
      <ColumnConstraints minWidth="10.0" prefWidth="100.0" />
      <ColumnConstraints maxWidth="1.0" minWidth="0.0" prefWidth="0.0" />
      <ColumnConstraints maxWidth="187.0" minWidth="186.0" prefWidth="187.0" />
      <ColumnConstraints />
   </columnConstraints>
   <rowConstraints>
      <RowConstraints minHeight="10.0" prefHeight="20"/>
      <RowConstraints minHeight="10.0" prefHeight="20"/>
   </rowConstraints>
</GridPane>
